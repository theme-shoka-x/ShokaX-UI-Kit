{
  "name": "valaxy-theme-shokax-repo",
  "type": "module",
  "private": true,
  "packageManager": "bun@1.3.0",
  "homepage": "https://valaxy.site",
  "scripts": {
    "build": "bun run build:demo",
    "build:demo": "bun run --cwd demo build",
    "ci:publish": "bun workspaces foreach --recursive -- bun publish --access public --no-git-checks",
    "dev": "bun run demo",
    "demo": "bun run --cwd demo dev",
    "lint": "eslint .",
    "release": "cd theme && git add -A && bumpp",
    "typecheck": "vue-tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "dayjs": "^1.11.19",
    "es-toolkit": "^1.42.0",
    "shokax-uikit": "workspace:*",
    "valaxy-theme-shokax": "workspace:*",
    "vue": "^3.5.25"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.4.2",
    "@unocss/eslint-plugin": "^66.5.10",
    "bumpp": "^10.3.2",
    "eslint": "^9.39.1",
    "eslint-plugin-format": "^1.1.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15",
    "vue-tsc": "^3.1.6"
  }
}
